#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nuc/clawd/projects/voice-chat/extensions/discord-voice/node_modules/.pnpm/@discordjs+node-pre-gyp@0.4.5/node_modules/@discordjs/node-pre-gyp/bin/node_modules:/home/nuc/clawd/projects/voice-chat/extensions/discord-voice/node_modules/.pnpm/@discordjs+node-pre-gyp@0.4.5/node_modules/@discordjs/node-pre-gyp/node_modules:/home/nuc/clawd/projects/voice-chat/extensions/discord-voice/node_modules/.pnpm/@discordjs+node-pre-gyp@0.4.5/node_modules/@discordjs/node_modules:/home/nuc/clawd/projects/voice-chat/extensions/discord-voice/node_modules/.pnpm/@discordjs+node-pre-gyp@0.4.5/node_modules:/home/nuc/clawd/projects/voice-chat/extensions/discord-voice/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nuc/clawd/projects/voice-chat/extensions/discord-voice/node_modules/.pnpm/@discordjs+node-pre-gyp@0.4.5/node_modules/@discordjs/node-pre-gyp/bin/node_modules:/home/nuc/clawd/projects/voice-chat/extensions/discord-voice/node_modules/.pnpm/@discordjs+node-pre-gyp@0.4.5/node_modules/@discordjs/node-pre-gyp/node_modules:/home/nuc/clawd/projects/voice-chat/extensions/discord-voice/node_modules/.pnpm/@discordjs+node-pre-gyp@0.4.5/node_modules/@discordjs/node_modules:/home/nuc/clawd/projects/voice-chat/extensions/discord-voice/node_modules/.pnpm/@discordjs+node-pre-gyp@0.4.5/node_modules:/home/nuc/clawd/projects/voice-chat/extensions/discord-voice/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@discordjs+node-pre-gyp@0.4.5/node_modules/@discordjs/node-pre-gyp/bin/node-pre-gyp" "$@"
else
  exec node  "$basedir/../../../../../../@discordjs+node-pre-gyp@0.4.5/node_modules/@discordjs/node-pre-gyp/bin/node-pre-gyp" "$@"
fi
